{
  "name": "flights-inventory-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "devextreme": "21.1",
    "devextreme-react": "21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.0",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "autoprefixer": "^9",
    "craco-alias": "^3.0.1",
    "dayjs": "^1.10.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "faker": "^5.5.3",
    "husky": ">=6",
    "json-server": "^0.16.3",
    "lint-staged": ">=10",
    "msw": "^0.34.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "prettier": "^2.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start:server": "json-server json-server/db.js --port 4000",
    "start:client": "craco start",
    "start": "run-p start:*",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix && npm test -- --coverage --watchAll=false",
    "*.{js,ts,tsx,scss,md}": "prettier --write"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/features/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 75,
        "statements": 75,
        "branches": 75,
        "functions": 75
      }
    }
  },
  "proxy": "http://localhost:4000"
}
